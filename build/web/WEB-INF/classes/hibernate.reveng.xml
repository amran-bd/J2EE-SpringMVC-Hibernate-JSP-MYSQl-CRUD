<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-schema="HR"/>
   <type-mapping>
      <sql-type jdbc-type="OTHER" hibernate-type="java.sql.Timestamp" />
      <sql-type jdbc-type="VARCHAR" length='1' hibernate-type="boolean" />
      <sql-type jdbc-type="BLOB"  hibernate-type="binary" />
      <sql-type jdbc-type="NUMERIC" precision="12" scale="2"  hibernate-type="java.lang.Double"/>
      <sql-type jdbc-type="NUMERIC" precision="5" scale="2"  hibernate-type="java.lang.Double"/>
      <sql-type jdbc-type="NUMERIC" precision="3" scale="0"  hibernate-type="java.lang.Short"/>
      <sql-type jdbc-type="NUMERIC" precision="4" scale="0"  hibernate-type="java.lang.Short"/>
      <sql-type jdbc-type="NUMERIC" hibernate-type="java.lang.Long"/>
  </type-mapping>
  <table-filter match-name="PRODUCT"/>
</hibernate-reverse-engineering>
